{{#each beers }}
<div class="random-beer">
<div class="random-beer-img"><img src={{this.image_url}} class="card-img-top" alt="..."></div>
  
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text">{{this.tagline}}</p>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text"><ul>{{#each this.food_pairing }} <li>{{ this }}</li> {{/each }}</ul></p>
    <p class="card-text">{{this.brewers_tips}}</p>
  </div>

</div>
{{/each }}